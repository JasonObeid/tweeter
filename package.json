{
  "name": "tweeter",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "e2e": "npx playwright test --headed",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint src --fix",
    "format": "prettier --write src",
    "fix": "yarn lint && yarn format",
    "predeploy": "yarn fix && yarn tsc",
    "deploy": "vercel --prod"
  },
  "dependencies": {
    "@supabase/supabase-js": "^1.30.3",
    "logrocket": "^2.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.34.15",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "use-debounce": "^7.0.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.19.1",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vitejs/plugin-react": "^1.0.7",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.23",
    "twitter-api-v2": "^1.11.0",
    "typescript": "^4.5.4",
    "vercel": "^24.0.0",
    "vite": "^2.8.0",
    "vitest": "^0.4.2"
  }
}
